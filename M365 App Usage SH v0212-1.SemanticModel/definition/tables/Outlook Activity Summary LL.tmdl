	column 'Total Outlook Activity Count'
		formatString: 0
		lineageTag: 4051eea2-2b80-4644-b449-68f64f0b3612
		summarizeBy: sum
		isNameInferred
		sourceColumn: [Total Outlook Activity Count]

		annotation SummarizationSetBy = Automatic

	column 'Outlook Tier' =
			
			VAR RawScore = [Total Outlook Activity Count]
			VAR P90 = PERCENTILEX.INC(
			    FILTER(ALL('Outlook Activity Summary LL'), NOT(ISBLANK([Total Outlook Activity Count]))),
			    [Total Outlook Activity Count],
			    0.9
			)
			VAR P75 = PERCENTILEX.INC(
			    FILTER(ALL('Outlook Activity Summary LL'), NOT(ISBLANK([Total Outlook Activity Count]))),
			    [Total Outlook Activity Count],
			    0.75
			)
			VAR P50 = PERCENTILEX.INC(
			    FILTER(ALL('Outlook Activity Summary LL'), NOT(ISBLANK([Total Outlook Activity Count]))),
			    [Total Outlook Activity Count],
			    0.5
			)
			RETURN
			IF(
			    ISBLANK(RawScore) || RawScore = 0,
			    BLANK(),
			    SWITCH(
			        TRUE(),
			        RawScore >= P90, 1,
			        RawScore >= P75, 2,
			        RawScore >= P50, 3,
			        4
			    )
			)
		formatString: 0
		lineageTag: d5a22f35-e296-4b8e-906b-b6086accba4b
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column 'Outlook Tier Label' =
			
			SWITCH(
			    [Outlook Tier],
			    1, "Tier 1 (Top 10%)",
			    2, "Tier 2 (10-25%)",
			    3, "Tier 3 (25-50%)",
			    4, "Tier 4 (Remaining 50%)",
			    BLANK()
			)
		lineageTag: 5c394236-a180-4e5d-a130-8e36d6cf38ee
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	partition 'Outlook Activity Summary LL' = calculated
		mode: import
		source = ```
				
				SUMMARIZE (
				    FILTER(SecDemoM365Usage, SecDemoM365Usage[Workload] = "Exchange"),
				    [UserId],
				    "Send Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[Operation] = "Send"
				        ),
				    "MailItemsAccessed Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[Operation] = "MailItemsAccessed"
				        ),
				
				    "Total Outlook Activity Count", COUNTROWS(SecDemoM365Usage)
				)
				```

	annotation PBI_Id = 2c7e9d5aaf8747febee35ec7ba96bd29

