	partition 'PowerPoint Activity by App Summary LL' = calculated
		mode: import
		source =
				
				SUMMARIZE (
				    FILTER(SecDemoM365Usage, LOWER(SecDemoM365Usage[SourceFileExtension]) IN { "pptx", "ppt", "ppsx" }),
				    [UserId],
				    "PowerPoint Web Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "PowerPoint Online", "PowerPoint for the web", "WebPowerPoint", "Office Online" }
				        ),
				    "PowerPoint Windows Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Microsoft PowerPoint", "PowerPoint" }
				        ),
				    "PowerPoint Mac Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "PowerPoint for Mac", "Microsoft PowerPoint for Mac" }
				        ),
				    "PowerPoint iOS Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "PowerPoint Mobile", "Microsoft PowerPoint for iOS" }
				        ),
				    "PowerPoint Android Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "PowerPoint for Android", "Microsoft PowerPoint for Android" }
				        ),
				    "Unknown Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            NOT(SecDemoM365Usage[ApplicationDisplayName] IN { "PowerPoint Online", "PowerPoint for the web", "WebPowerPoint", "Office Online", "Microsoft PowerPoint", "PowerPoint", "PowerPoint for Mac", "Microsoft PowerPoint for Mac", "PowerPoint Mobile", "Microsoft PowerPoint for iOS", "PowerPoint for Android", "Microsoft PowerPoint for Android" })
				        ),
				    "PowerPoint Total Activity", COUNTROWS(SecDemoM365Usage)
				)

	annotation PBI_Id = 989fe16fcc78404897459ebefb2e39b3

