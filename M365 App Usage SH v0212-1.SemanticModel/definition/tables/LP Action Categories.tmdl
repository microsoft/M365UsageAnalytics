/// Disconnected table with the 4 action category labels for the distribution chart
table 'LP Action Categories'
	lineageTag: b1c2d3e4-f5a6-b7c8-d9e0-f1a2b3c4d5e6

	column ActionName
		lineageTag: c1d2e3f4-a5b6-c7d8-e9f0-a1b2c3d4e5f6
		isNameInferred
		sourceColumn: [ActionName]
		sortByColumn: ActionOrder

	column ActionOrder
		isHidden
		lineageTag: d1e2f3a4-b5c6-d7e8-f9a0-b1c2d3e4f5a6
		isNameInferred
		sourceColumn: [ActionOrder]

	column ActionColor
		isHidden
		lineageTag: e1f2a3b4-c5d6-e7f8-a9b0-c1d2e3f4a5b6
		isNameInferred
		sourceColumn: [ActionColor]

	partition 'LP Action Categories' = calculated
		mode: import
		source =
				DATATABLE(
				    "ActionName", STRING,
				    "ActionOrder", INTEGER,
				    "ActionColor", STRING,
				    {
				        {"License First", 1, "#2E7D32"},
				        {"License Next", 2, "#1565C0"},
				        {"Potential", 3, "#F9A825"},
				        {"Developing", 4, "#BDBDBD"}
				    }
				)

