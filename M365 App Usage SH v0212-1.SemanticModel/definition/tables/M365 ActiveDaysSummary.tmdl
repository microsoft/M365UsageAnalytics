	partition 'M365 ActiveDaysSummary' = calculated
		mode: import
		source = ```
				
				VAR BaseTable = 
				    ADDCOLUMNS(
				        SUMMARIZE(
				            'SecDemoM365Usage',
				            'SecDemoM365Usage'[UserId]
				        ),
				        "TeamsActiveDays", 
				            CALCULATE(
				                DISTINCTCOUNT('SecDemoM365Usage'[CreationDate]),
				                'SecDemoM365Usage'[Workload] = "MicrosoftTeams",
				                'SecDemoM365Usage'[Operation] IN {"MessageSent", "MessageRead", "MeetingParticipantJoined"}
				            ),
				        "OutlookActiveDays",
				            CALCULATE(
				                DISTINCTCOUNT('SecDemoM365Usage'[CreationDate]),
				                'SecDemoM365Usage'[Workload] = "Exchange",
				                'SecDemoM365Usage'[Operation] IN {"Send", "MailItemsAccessed"}
				            ),
				        "WordActiveDays",
				            CALCULATE(
				                DISTINCTCOUNT('SecDemoM365Usage'[CreationDate]),
				                LOWER('SecDemoM365Usage'[SourceFileExtension]) IN {"docx", "doc", "dotx"},
				                'SecDemoM365Usage'[Operation] IN {"FileViewed", "FileModified", "FileDownloaded", "FileUploaded"}
				            ),
				        "ExcelActiveDays",
				            CALCULATE(
				                DISTINCTCOUNT('SecDemoM365Usage'[CreationDate]),
				                LOWER('SecDemoM365Usage'[SourceFileExtension]) IN {"xlsx", "xls", "xlsm", "csv"},
				                'SecDemoM365Usage'[Operation] IN {"FileViewed", "FileModified", "FileDownloaded", "FileUploaded"}
				            ),
				        "PowerPointActiveDays",
				            CALCULATE(
				                DISTINCTCOUNT('SecDemoM365Usage'[CreationDate]),
				                LOWER('SecDemoM365Usage'[SourceFileExtension]) IN {"pptx", "ppt", "ppsx"},
				                'SecDemoM365Usage'[Operation] IN {"FileViewed", "FileModified", "FileDownloaded", "FileUploaded"}
				            ),
				        "TeamsActivityCount",
				            CALCULATE(
				                COUNTROWS('SecDemoM365Usage'),
				                'SecDemoM365Usage'[Workload] = "MicrosoftTeams",
				                'SecDemoM365Usage'[Operation] IN {"MessageSent", "MessageRead", "MeetingParticipantJoined"}
				            ),
				        "OutlookActivityCount",
				            CALCULATE(
				                COUNTROWS('SecDemoM365Usage'),
				                'SecDemoM365Usage'[Workload] = "Exchange",
				                'SecDemoM365Usage'[Operation] IN {"Send", "MailItemsAccessed", "MailboxLogin"}
				            ),
				        "OfficeFilesActivityCount",
				            CALCULATE(
				                COUNTROWS('SecDemoM365Usage'),
				                LOWER('SecDemoM365Usage'[SourceFileExtension]) IN {"docx", "doc", "dotx", "xlsx", "xls", "xlsm", "csv", "pptx", "ppt", "ppsx"},
				                'SecDemoM365Usage'[Operation] IN {"FileViewed", "FileModified", "FileDownloaded", "FileUploaded"}
				            ),
				        "Copilot License Status",
				              VAR UserEmail = 'SecDemoM365Usage'[UserId]
				              RETURN
				             IF(
				                  LOOKUPVALUE(
				                     'SecDemEntraUsers'[hasLicense],
				                     'SecDemEntraUsers'[UserPrincipalName], UserEmail
				             ) = TRUE(),
				             "Licensed",
				             "Unlicensed"
				    )
				            )
				
				RETURN
				ADDCOLUMNS(
				    FILTER(
				        BaseTable,
				        [TeamsActiveDays] + [OutlookActiveDays] + [WordActiveDays] + [ExcelActiveDays] + [PowerPointActiveDays] > 0
				    ),
				    "Teams Activity Segment",
				        IF(
				            [TeamsActiveDays] > 0,
				            SWITCH(
				                TRUE(),
				                [TeamsActiveDays] <= 5, "1. 1-5 Days (Infrequent)",
				                [TeamsActiveDays] <= 10, "2. 6-10 Days (Moderate)",
				                [TeamsActiveDays] <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        ),
				    "Outlook Activity Segment",
				        IF(
				            [OutlookActiveDays] > 0,
				            SWITCH(
				                TRUE(),
				                [OutlookActiveDays] <= 5, "1. 1-5 Days (Infrequent)",
				                [OutlookActiveDays] <= 10, "2. 6-10 Days (Moderate)",
				                [OutlookActiveDays] <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        ),
				    "Word Activity Segment",
				        IF(
				            [WordActiveDays] > 0,
				            SWITCH(
				                TRUE(),
				                [WordActiveDays] <= 5, "1. 1-5 Days (Infrequent)",
				                [WordActiveDays] <= 10, "2. 6-10 Days (Moderate)",
				                [WordActiveDays] <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        ),
				    "Excel Activity Segment",
				        IF(
				            [ExcelActiveDays] > 0,
				            SWITCH(
				                TRUE(),
				                [ExcelActiveDays] <= 5, "1. 1-5 Days (Infrequent)",
				                [ExcelActiveDays] <= 10, "2. 6-10 Days (Moderate)",
				                [ExcelActiveDays] <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        ),
				    "PowerPoint Activity Segment",
				        IF(
				            [PowerPointActiveDays] > 0,
				            SWITCH(
				                TRUE(),
				                [PowerPointActiveDays] <= 5, "1. 1-5 Days (Infrequent)",
				                [PowerPointActiveDays] <= 10, "2. 6-10 Days (Moderate)",
				                [PowerPointActiveDays] <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        ),
				    "Office Files Activity Segment",
				        VAR OfficeActiveDays = [WordActiveDays] + [ExcelActiveDays] + [PowerPointActiveDays]
				        RETURN
				        IF(
				            OfficeActiveDays > 0,
				            SWITCH(
				                TRUE(),
				                OfficeActiveDays <= 5, "1. 1-5 Days (Infrequent)",
				                OfficeActiveDays <= 10, "2. 6-10 Days (Moderate)",
				                OfficeActiveDays <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        ),
				    "Overall M365 Activity Segment",
				        VAR TotalActiveDays = [TeamsActiveDays] + [OutlookActiveDays] + [WordActiveDays] + [ExcelActiveDays] + [PowerPointActiveDays]
				        RETURN
				        IF(
				            TotalActiveDays > 0,
				            SWITCH(
				                TRUE(),
				                TotalActiveDays <= 5, "1. 1-5 Days (Infrequent)",
				                TotalActiveDays <= 10, "2. 6-10 Days (Moderate)",
				                TotalActiveDays <= 19, "3. 11-19 Days (Frequent)",
				                "4. 20+ Days (Daily)"
				            ),
				            "0. No Usage"
				        )
				)
				```

