	measure '% of User Distribution by Priority' = ```
			
			VAR TotalUsers = 
			    CALCULATE(
			        DISTINCTCOUNT('SecDemoM365Usage'[UserId]),
			        ALLSELECTED('SecDemoM365Usage')
			    )
			VAR UsersInCurrentContext = 
			    DISTINCTCOUNT('SecDemoM365Usage'[UserId])
			RETURN
			    DIVIDE(UsersInCurrentContext, TotalUsers, 0)
