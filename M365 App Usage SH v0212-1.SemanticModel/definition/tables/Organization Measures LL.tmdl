	measure 'Total Users in Tiers 1-3' =
			
			VAR SelectedActivity = SELECTEDVALUE('Activity Type'[Activity], "Overall M365 Activity")
			RETURN
			    CALCULATE(
			        COUNTROWS('SecDemEntraUsers'),
			        FILTER(
			            'SecDemEntraUsers',
			            SWITCH(
			                SelectedActivity,
			                "Overall M365 Activity", [M365 All Apps Tier V2] IN {1,2,3},
			                "Teams Activity", [Teams Tier v2] IN {1,2,3},
			                "Ooutlook Activity", [Outlook Tier v2] IN {1,2,3},
			                "WordActivity", [Word Tier V2] IN {1,2,3},
			                "Excel Activity", [Excel Tier V2] IN {1,2,3},
			                "PPT Activity", [PowerPoint Tier V2] IN {1,2,3},
			                FALSE()
			            )
			        )
			    )
		formatString: 0
		lineageTag: 7ddf9ab3-24e1-4f92-9e3c-8706699914f1

