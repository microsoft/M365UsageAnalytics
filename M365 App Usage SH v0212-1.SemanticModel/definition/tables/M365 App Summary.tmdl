table 'M365 App Summary'
	lineageTag: ef9011fd-ae28-4538-b566-7780936866c5

	column SourceFileExtension
		lineageTag: 5dd12e5f-ad2b-42de-b467-fcc8278816e6
		summarizeBy: none
		isNameInferred
		sourceColumn: SecDemoM365Usage[SourceFileExtension]

		annotation SummarizationSetBy = Automatic

	column 'M365 App'
		lineageTag: 96d97c79-56a9-48b6-ace4-5838f6f3c472
		summarizeBy: none
		isNameInferred
		sourceColumn: [M365 App]

		annotation SummarizationSetBy = Automatic

	partition 'M365 App Summary' = calculated
		mode: import
		source = ```
				
				SUMMARIZE(
				    'SecDemoM365Usage',
				    [SourceFileExtension],
				    "M365 App",
				    VAR ext = LOWER([SourceFileExtension])
				    RETURN 
				    SWITCH(TRUE(), 
				        ext IN { "docx", "doc", "dotx" }, "Word", 
				        ext IN { "xlsx", "xls", "xlsm", "csv" }, "Excel", 
				        ext IN { "pptx", "ppt", "ppsx" }, "PowerPoint", 
				        "Other" 
				    )
				)
				```

	annotation PBI_Id = 72d07cf1372641d39c9947d687bada44

