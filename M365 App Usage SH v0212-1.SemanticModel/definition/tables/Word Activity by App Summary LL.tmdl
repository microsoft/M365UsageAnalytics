	partition 'Word Activity by App Summary LL' = calculated
		mode: import
		source =
				
				SUMMARIZE (
				    FILTER(SecDemoM365Usage, LOWER(SecDemoM365Usage[SourceFileExtension]) IN { "docx", "doc", "dotx" }),
				    [UserId],
				    "Word Web Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Word Online", "Word for the web", "WebWord", "Office Online" }
				        ),
				    "Word Windows Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Microsoft Word", "Word" }
				        ),
				    "Word Mac Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Word for Mac", "Microsoft Word for Mac" }
				        ),
				    "Word iOS Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Word Mobile", "Microsoft Word for iOS" }
				        ),
				    "Word Android Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Word for Android", "Microsoft Word for Android" }
				        ),
				    "Unknown Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            NOT(SecDemoM365Usage[ApplicationDisplayName] IN { "Word Online", "Word for the web", "WebWord", "Office Online", "Microsoft Word", "Word", "Word for Mac", "Microsoft Word for Mac", "Word Mobile", "Microsoft Word for iOS", "Word for Android", "Microsoft Word for Android" })
				        ),
				    "Word Total Activity", COUNTROWS(SecDemoM365Usage)
				)

	annotation PBI_Id = d073f2f908e94e51a194d604680b2b20

