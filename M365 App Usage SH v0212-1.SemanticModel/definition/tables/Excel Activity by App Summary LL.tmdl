	partition 'Excel Activity by App Summary LL' = calculated
		mode: import
		source =
				
				SUMMARIZE (
				    FILTER(SecDemoM365Usage, LOWER(SecDemoM365Usage[SourceFileExtension]) IN { "xlsx", "xls", "xlsm", "csv" }),
				    [UserId],
				    "Excel Web Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Excel Online", "Excel for the web", "WebExcel", "Office Online" }
				        ),
				    "Excel Windows Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Microsoft Excel", "Excel" }
				        ),
				    "Excel Mac Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Excel for Mac", "Microsoft Excel for Mac" }
				        ),
				    "Excel iOS Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Excel Mobile", "Microsoft Excel for iOS" }
				        ),
				    "Excel Android Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[ApplicationDisplayName] IN { "Excel for Android", "Microsoft Excel for Android" }
				        ),
				    "Unknown Count",
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            NOT(SecDemoM365Usage[ApplicationDisplayName] IN { "Excel Online", "Excel for the web", "WebExcel", "Office Online", "Microsoft Excel", "Excel", "Excel for Mac", "Microsoft Excel for Mac", "Excel Mobile", "Microsoft Excel for iOS", "Excel for Android", "Microsoft Excel for Android" })
				        ),
				    "Excel Total Activity", COUNTROWS(SecDemoM365Usage)
				)

	annotation PBI_Id = ab110a94b5ca47f29f07b7a861d82d6c

