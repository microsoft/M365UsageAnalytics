	column 'PowerPoint Tier' =
			
			VAR RawScore = [PowerPoint Total Activity]
			VAR P90 = PERCENTILEX.INC(
			    FILTER(ALL('PowerPoint Activity Summary LL'), NOT(ISBLANK([PowerPoint Total Activity]))),
			    [PowerPoint Total Activity],
			    0.9
			)
			VAR P75 = PERCENTILEX.INC(
			    FILTER(ALL('PowerPoint Activity Summary LL'), NOT(ISBLANK([PowerPoint Total Activity]))),
			    [PowerPoint Total Activity],
			    0.75
			)
			VAR P50 = PERCENTILEX.INC(
			    FILTER(ALL('PowerPoint Activity Summary LL'), NOT(ISBLANK([PowerPoint Total Activity]))),
			    [PowerPoint Total Activity],
			    0.5
			)
			RETURN
			IF(
			    ISBLANK(RawScore) || RawScore = 0,
			    BLANK(),
			    SWITCH(
			        TRUE(),
			        RawScore >= P90, 1,
			        RawScore >= P75, 2,
			        RawScore >= P50, 3,
			        4
			    )
			)
		formatString: 0
		lineageTag: 53d3517d-6612-4449-860b-0c2984dad6d2
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column 'PowerPoint Tier Label' =
			
			SWITCH(
			    [PowerPoint Tier],
			    1, "Tier 1 (Top 10%)",
			    2, "Tier 2 (10-25%)",
			    3, "Tier 3 (25-50%)",
			    4, "Tier 4 (Remaining 50%)",
			    BLANK()
			)
		lineageTag: 4a1cd651-7ec2-4b29-acac-9c431d91335b
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	partition 'PowerPoint Activity Summary LL' = calculated
		mode: import
		source = ```
				
				SUMMARIZE (
				    FILTER(SecDemoM365Usage, LOWER(SecDemoM365Usage[SourceFileExtension]) IN { "pptx", "ppt", "ppsx" }),
				    [UserId],
				    "PowerPoint Viewed Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[Operation] = "FileViewed"
				        ),
				    "PowerPoint Modified Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[Operation] = "FileModified"
				        ),
				    "PowerPoint Downloaded Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[Operation] = "FileDownloaded"
				        ),
				    "PowerPoint Uploaded Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            SecDemoM365Usage[Operation] = "FileUploaded"
				        ),
				    "PowerPoint Other Operations Count", 
				        CALCULATE(
				            COUNTROWS(SecDemoM365Usage),
				            NOT(SecDemoM365Usage[Operation] IN { "FileViewed", "FileModified", "FileDownloaded", "FileUploaded" })
				        ),
				    "PowerPoint Total Activity", COUNTROWS(SecDemoM365Usage)
				)
				```

