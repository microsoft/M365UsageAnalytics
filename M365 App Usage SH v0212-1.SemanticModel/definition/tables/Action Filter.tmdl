	measure 'Debug License Values' =
			
			VAR TrueCount = CALCULATE(COUNTROWS(SecDemEntraUsers), SecDemEntraUsers[hasLicense] = "True")
			VAR FalseCount = CALCULATE(COUNTROWS(SecDemEntraUsers), SecDemEntraUsers[hasLicense] = "False")
			VAR BlankCount = CALCULATE(COUNTROWS(SecDemEntraUsers), ISBLANK(SecDemEntraUsers[hasLicense]))
			RETURN
			"True:" & TrueCount & " False:" & FalseCount & " Blank:" & BlankCount
